	<script language="VBScript">
		Sub Window_OnLoad
			Window.ResizeTo 1000,640
			show_result()
		End Sub
		
		Dim objFSO
		Dim objFolder
		Dim R
	    Set objFSO = CreateObject("Scripting.FileSystemObject")

		Function listup_files()
			Dim strFiles
			Dim File
			For Each File In objFolder.Files
				strFiles = strFiles & File.Name & vbCrLf
			Next
			listup_files = strFiles
		End Function

	    Sub show_result()
			Dim strResult
			Set objFolder = objFSO.GetFolder("LOG")
			strResult = listup_files
	        document.getElementById("write").value = strResult
	    End Sub
	</script>
